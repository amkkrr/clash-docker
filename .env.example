# Clash Core Configuration
CLASH_SECRET=your_clash_password_here
CLASH_HTTP_PORT=7890
CLASH_SOCKS_PORT=7891
CLASH_CONTROL_PORT=9090
CLASH_EXTERNAL_CONTROLLER=0.0.0.0:9090
CLASH_ALLOW_LAN=false
CLASH_LOG_LEVEL=info
CLASH_IPV6=false
CLASH_INTERFACE_NAME=WLAN

# JP Hysteria2 Proxy
JP_HYSTERIA2_SERVER=your_jp_server.com
JP_HYSTERIA2_PORTS=60000-65000
JP_HYSTERIA2_PASSWORD=your_jp_hysteria2_password

# SJC Hysteria2 Proxy
SJC_HYSTERIA2_SERVER=your_sjc_server.com
SJC_HYSTERIA2_PORTS=60000-65000
SJC_HYSTERIA2_PASSWORD=your_sjc_hysteria2_password

# SJC Landing SS Proxy
SJC_LANDING_SS_SERVER=your_sjc_landing_server.com
SJC_LANDING_SS_PORT=8388
SJC_LANDING_SS_PASSWORD=your_ss_password

# JP Direct VLESS Proxy
JP_DIRECT_SERVER=your_jp_direct_server.com
JP_DIRECT_PORT=443
JP_DIRECT_UUID=your_vless_uuid
JP_DIRECT_WS_PATH=/your_ws_path

# SJC Landing Hysteria2 Proxy
SJC_LANDING_HYSTERIA2_SERVER=your_sjc_landing_hysteria2_server.com
SJC_LANDING_HYSTERIA2_PORTS=60000-65000
SJC_LANDING_HYSTERIA2_PASSWORD=your_sjc_landing_hysteria2_password

# SJC Direct VMess Proxy
SJC_DIRECT_SERVER=your_sjc_direct_server.com
SJC_DIRECT_PORT=443
SJC_DIRECT_UUID=your_vmess_uuid
SJC_DIRECT_WS_PATH=/your_ws_path

# SJC Landing Direct VMess Proxy
SJC_LANDING_DIRECT_SERVER=your_sjc_landing_direct_server.com
SJC_LANDING_DIRECT_PORT=443
SJC_LANDING_DIRECT_UUID=your_landing_vmess_uuid
